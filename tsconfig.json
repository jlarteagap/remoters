{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*":["src/component/*"],
      "@service/*":["service/*"],
      "@context*": ["src/context/*"],
      "@utils/*": ["src/utils/*"],
      "@public/*": ["public/*"],
      "@hoc/*": ["src/hoc/*"]
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "ES2021.String"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "service/graphql", "src/component/add/ubications/Ubications.jsx", "service/graphql", "src/component/dashboard/LastJobs.jsx", "src/component/dashboard/LastJobs.jsx", "service/graphql"  ],
  "exclude": [
    "node_modules"
  ]
}
